extend base

block content
  main.main
    .login-form
      h2.heading-secondary.ma-bt-lg Log into your account
      form.form.form--sms(data-service-id=`${serviceId}`)
        .form__group
          label.form__label(for='sms') SMS code
          input#sms.form__input(type='text', required)
        .form__group
          button.btn.btn--green Login